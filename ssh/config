# Kan måske gøre forbindelsen til dtu-serveren bedre
Host *
   ServerAliveCountMax 120
   ServerAliveInterval 30

Host desktop
   User paw
   HostName arch_home
   IdentityFile ~/.ssh/laptop_desktop_sync

Host hpc
   User s082705
   HostName hpc-fe.gbar.dtu.dk
   IdentityFile ~/.ssh/dtumnt

Host dtu
   User s082705
   HostName login.gbar.dtu.dk
   IdentityFile ~/.ssh/dtumnt

# Default GitHub user (paw)
Host github.com
  HostName github.com
  User git
  IdentityFile /home/paw/.ssh/github

